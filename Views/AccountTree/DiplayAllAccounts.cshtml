@model IEnumerable<Small_ERP.Models.AccountTree>

@{
    ViewData["Title"] = "DiplayAllAccounts";
}

<h1>All Accounts</h1>


<table class="table table-bordered table-striped">
    <thead>
        <tr>
            
            <th>
                Account Id
            </th>
            <th>
                Account Name 
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.Acc_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acc_Name)
            </td>
            
        </tr>
}
    </tbody>
</table>
